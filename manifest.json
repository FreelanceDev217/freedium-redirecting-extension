{
  "manifest_version": 3,
  "name": "Freedium Redirect",
  "version": "1.0",
  "description": "Redirects paywalled Medium articles to Freedium",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "*://medium.com/*",
        "*://*.medium.com/*",
        "*://*.datadriveninvestor.com/*",
        "*://*.towardsai.net/*",
        "*://*.plainenglish.io/*",
        "*://*.betterprogramming.pub/*",
        "*://*.levelup.gitconnected.com/*",
        "*://*.blog.devgenius.io/*",
        "*://*.bootcamp.uxdesign.cc/*",
        "*://*.blog.bitsrc.io/*",
        "*://*.blog.usejournal.com/*",
        "*://*.blog.prototypr.io/*",
        "*://*.blog.angular-university.io/*",
        "*://*.itnext.io/*",
        "*://*.codeburst.io/*",
        "*://*.proandroiddev.com/*",
        "*://*.blog.kotlin-academy.com/*",
        "*://*.blog.coinbase.com/*",
        "*://*.blog.discord.com/*",
        "*://*.netflixtechblog.com/*",
        "*://*.blog.tensorflow.org/*",
        "*://*.blog.logrocket.com/*",
        "*://*.blog.insightdatascience.com/*",
        "*://*.betterhumans.pub/*",
        "*://*.thinkgrowth.org/*",
        "*://*.uxdesign.cc/*",
        "*://*.writingcooperative.com/*",
        "*://*.blog.cleancoder.com/*"
      ],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}